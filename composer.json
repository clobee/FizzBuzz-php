{
    "name": "php_exercices/fizz-buzz",
    "type": "project",
    "require": {
        "php": "7.4",
        "phpstan/phpstan": "^0.12.11"
    },
    "autoload": {
        "psr-4": {
            "php_exercices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "php_exercices_tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "scripts": {
        "test": [
            "./vendor/phpunit/phpunit/phpunit",
            "./vendor/phpstan/phpstan/phpstan analyse -l max src tests"
        ]
    }
}
